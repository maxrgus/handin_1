package com.srebmem;

import java.util.*;
import java.io.*;

public class Main {

    public static void main(String[] args)
	throws IOException {
	Scanner sc = new Scanner(new File("com/srebmem/medlemsregister.txt"));
	sc.useDelimiter(";|\\n");
	List<Member> members = readFile(sc);
	System.out.println(members);
	

    }
    public static int countTokens(Scanner sc) {
	String s = sc.nextLine();
	StringTokenizer tokens = new StringTokenizer(s, ";");
	return tokens.countTokens();
    }
    public static List<Member> readFile(Scanner sc)
	throws IOException {
	String[] input = new String[8];
	List<Member> members = new ArrayList<Member>();
	while (sc.hasNext()) {
	    for (int i=0; i <= 7; i++) {
		input[i] = sc.next();
	    }
	    int id = Integer.parseInt(input[0]);
	    int role = Integer.parseInt(input[5]);
	    int gender = Integer.parseInt(input[7]);
	    members.add(new Member(id, input[1], input[2], input[3],
				   input[4], role, input[6], gender));
	}
	return members;
    }
    

}
